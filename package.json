{
  "name": "workspace",
  "version": "1.0.0",
  "description": "## 소프트웨어 스택",
  "scripts": {
    "install-all": "npm install && npm run install-frontend && npm run install-backend install-pre-commit-hook",
    "install-frontend": "cd frontend && npm install",
    "install-backend": "cd backend && npm install",
    "install-pre-commit-hook": "cp -avp tools/pre-commit .git/hooks/",
    "serve-all": "npm-run-all -p serve-frontend serve-backend",
    "serve-frontend": "cd frontend && npm run serve",
    "serve-backend": "cd backend && npm run serve",
    "update-readme-toc": "doctoc --github --notitle README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lhwn5407/naver-movie-api-test.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/lhwn5407/naver-movie-api-test/issues"
  },
  "homepage": "https://github.com/lhwn5407/naver-movie-api-test#readme",
  "devDependencies": {
    "@vue/eslint-config-prettier": "^6.0.0",
    "babel-eslint": "^10.1.0",
    "doctoc": "^1.4.0",
    "eslint": "^7.10.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-vue": "^7.0.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2"
  }
}
